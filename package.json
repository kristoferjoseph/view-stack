{
  "name": "view-stack",
  "version": "3.0.0",
  "description": "Router for stacking views.",
  "main": "index.js",
  "scripts": {
    "test": "npm run browser-tests",
    "build": "browserify ./index.js -o ./bundle.js",
    "build-tests": "browserify ./test.js -o ./public/test.bundle.js",
    "watch-tests": "watchify ./test.js -o ./public/test.bundle.js",
    "browser-tests": "npm start && npm run build-tests && open http://localhost:6661",
    "start": "node server.js",
    "umd": "browserify ./index.js --standalone viewStack | uglifyjs --compress --mangle --screw-ie8 > view-stack.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristoferjoseph/view-stack.git"
  },
  "keywords": [
    "router",
    "view",
    "stack",
    "layer",
    "component",
    "loader"
  ],
  "author": "@dam",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kristoferjoseph/view-stack/issues"
  },
  "homepage": "https://github.com/kristoferjoseph/view-stack#readme",
  "dependencies": {
    "thataway": "^2.1.2",
    "yo-yo": "^1.2.2"
  },
  "devDependencies": {
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "browserify": "^13.1.0",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "tape": "^4.6.0",
    "watchify": "^3.7.0",
    "yo-yoify": "^3.4.0"
  }
}
